<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="BarTitleUno" parent="@android:style/TextAppearance">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">#2196F3</item>
        <item name="android:textSize">25dp</item>
        <item name="android:padding">15dp</item>
    </style>

    <style name="TextoUno" parent="@android:style/TextAppearance">
        <item name="android:textColor">#A8A8A8</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:textSize">20dp</item>
    </style>

    <style name="Botonuno" parent="@android:style/TextAppearance">

    </style>



</resources>
